{
  "ConnectionStrings": {
    "DefaultConnection": "Host={Host};Port={Port};Database={Database};Username={User};Password={Password};SSLMode=Disable;TrustServerCertificate=True"
  },
  "Jwt": {
    "Key": "{AuthKey}",
    "Issuer": "{JwtIssuer}",
    "Audience": "{JwtAudience}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
